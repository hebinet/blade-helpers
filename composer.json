{
  "name": "hebinet/blade-helpers",
  "description": "Add includeOnce Blade directive",
  "keywords": [
    "hebinet",
    "blade-helpers"
  ],
  "homepage": "https://github.com/hebinet/blade-helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Hebenstreit",
      "email": "office@hebinet.at",
      "homepage": "https://www.hebinet.at",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/contracts": "~5.8.0||~6.0",
    "illuminate/http": "~5.8.0||~6.0",
    "illuminate/support": "~5.8.0||~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Hebinet\\BladeHelpers\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hebinet\\BladeHelpers\\tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hebinet\\BladeHelpers\\ServiceProvider"
      ]
    }
  }
}
